<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activity.MainActivity"
    android:gravity="center_horizontal">
    <LinearLayout
        android:layout_marginTop="60dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <TextView
            android:layout_width="match_parent"
            android:layout_height="69dp"
            android:id="@+id/text1">
        </TextView>

    </LinearLayout>
    <Button
        android:id="@+id/btn1"
        android:layout_width="100dp"
        android:layout_height="50dp"
        android:text="移除白名单">
    </Button>
    <Button
        android:id="@+id/btn2"
        android:layout_width="100dp"
        android:layout_height="50dp"
        android:text="添加白名单">
    </Button>
    <Button
        android:id="@+id/btn3"
        android:layout_width="100dp"
        android:layout_height="50dp"
        android:text="锁机">
    </Button>
    <Button
        android:id="@+id/btn4"
        android:layout_width="100dp"
        android:layout_height="50dp"
        android:text="解锁">
    </Button>
    <Button
        android:id="@+id/btn5"
        android:layout_width="150dp"
        android:layout_height="50dp"
        android:text="恢复出厂设置">
    </Button>
    <Button
        android:id="@+id/btn6"
        android:layout_width="150dp"
        android:layout_height="50dp"
        android:text="远程安装应用">
    </Button>
    <Button
        android:id="@+id/btn7"
        android:layout_width="150dp"
        android:layout_height="50dp"
        android:text="远程卸载应用42342342342 ">
    </Button>
    <Button
        android:id="@+id/btn8"
        android:layout_width="100dp"
        android:layout_height="50dp"
        android:text="下载图片">
    </Button>
    <Button
        android:id="@+id/btn9"
        android:layout_width="150dp"
        android:layout_height="50dp"
        android:text="获取通话时长"/>
    <Button
        android:id="@+id/btn10"
        android:layout_width="150dp"
        android:layout_height="50dp"
        android:text="跳转lock"/>

</LinearLayout>